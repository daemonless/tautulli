#!/bin/sh
# Tautulli s6 service (pkg version)

export HOME=/config
export TAUTULLI_DOCKER=True
export PYTHONDONTWRITEBYTECODE=1
export LC_ALL=C.UTF-8
export LANG=C.UTF-8

cd /usr/local/www/tautulli
exec /usr/local/bin/s6-setuidgid bsd /usr/local/bin/python3.11 Tautulli.py --nolaunch --datadir /config